; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Endless Waves"
config/description="A strategic tower defense roguelike mobile game. Defend your base, recruit units, and survive endless waves!"
config/version="0.1.0-alpha"
config/tags=PackedStringArray("tower_defense", "roguelike", "mobile", "strategy")
run/main_scene="res://scenes/main/Main.tscn"
config/features=PackedStringArray("4.4", "Mobile")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
config/icon="res://icon.svg"
boot_splash/show_image=false

[autoload]

GameManager="*res://autoloads/GameManager.gd"
EventBus="*res://autoloads/EventBus.gd"
AudioManager="*res://autoloads/AudioManager.gd"
SaveManager="*res://autoloads/SaveManager.gd"
DataLoader="*res://autoloads/DataLoader.gd"

[debug]

gdscript/warnings/enable=true
gdscript/warnings/treat_warnings_as_errors=false
gdscript/warnings/exclude_addons=true
